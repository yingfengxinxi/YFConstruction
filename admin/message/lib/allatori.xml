<config>
    <input>
        <jar in="message-0.0.1-SNAPSHOT.jar" out="message-0.0.1.jar"/>
    </input>

    <keep-names>
        <class access="protected+">
            <field access="protected+"/>
            <method access="protected+"/>
        </class>
    </keep-names>
    <!-- 忽略混肴 -->
    <ignore-classes>
        <class template="class *springframework*" />
        <!--<class template="class *core*" />
        <class template="class *MessageApplication*" />
        <class template="class *mapper*" />
        <class template="class *entity*" />-->
    </ignore-classes>

    <property name="log-file" value="log.xml"/>
</config>