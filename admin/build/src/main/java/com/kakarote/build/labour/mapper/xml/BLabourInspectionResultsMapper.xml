<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kakarote.build.labour.mapper.BLabourInspectionResultsMapper">

    <select id="getPid" resultType="com.kakarote.build.labour.entity.PO.BLabourInspectionResults">
        select * from b_labour_inspection_results
        where inspection_id = #{pid}
    </select>
</mapper>
