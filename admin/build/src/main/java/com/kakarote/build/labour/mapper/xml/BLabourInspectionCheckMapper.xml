<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kakarote.build.labour.mapper.BLabourInspectionCheckMapper">

    <select id="selectByInspectionId" resultType="com.kakarote.build.labour.entity.PO.BLabourInspectionCheck">
        select a.*
        from b_labour_inspection_check a
        where a.inspection_id = #{inspectionId}
    </select>
</mapper>
